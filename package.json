{
  "name": "one-hundred-dates-core",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "main": "index.js",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.5.5",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.10",
    "pg": "^8.4.0",
    "pusher": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.20"
  },
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "ts-node-dev src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "create:route": "node scripts/create_route.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate ./src/database/migrations/$npm_config_name -- -d ./src/database/data-source.ts",
    "empty-migration": "npm run typeorm migration:create ./src/database/migrations/$npm_config_name",
    "migration:run": "npm run typeorm migration:run -- -d ./src/database/data-source.ts",
    "migration:rollback": "npm run typeorm migration:revert -- -d ./src/database/data-source.ts"
  }
}
